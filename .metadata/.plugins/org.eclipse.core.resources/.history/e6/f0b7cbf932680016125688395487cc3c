     <script>
		function fn()
		{
			myname=na.value;
			myage=ag.value
			mybirth=bir.value
			data = {name:myname,age:myage,birth:mybirth};
			$('#mydiv').load("board2.jsp",data,comFn)
		}
		function comFn() {
			alert("complete")
		}
		window.onload = function()
		{
			  var tr = $('#mybody').empty()
				$.getJSON('board1.jsp',JFn)
		}
		function JFn(data) {
			for(n in data){
				$('#mytable > tbody:last').append('<tr><td>'+eval(Number(n)+1)+'</td><td>' +data[n].name+ '</td><td>'+data[n].contents+'</td><td>'+data[n].date+'</td></tr>');
		}
		}
	</script>

