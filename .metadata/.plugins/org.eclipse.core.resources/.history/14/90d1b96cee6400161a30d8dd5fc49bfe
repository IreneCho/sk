		<div id="mydiv">이동3</div>

	</div>

	<script>
		$(function() {
			$(document).on('swipe', "#pageone", function() {
				$.mobile.changePage("#pagetwo",
				{transition : 'slide'})
				
			})

			$(function() {
				$(document).on('swipe', "#pagetwo", function() {
					$.mobile.changePage("#pagethree",
					{transition : 'slide'})
				})
				
			$(function() {
				$(document).on('swipeleft', "#pagethree", function() {
					$.mobile.changePage("#pageone",
					{transition : 'slide'})
				})
				
			$(function() {
				$(document).on('swiperight', "#pagethree", function() {
					$.mobile.changePage("#pagetwo",
					{transition : 'slide'})
					

				})	
		})
		})
		
	
	</script>
