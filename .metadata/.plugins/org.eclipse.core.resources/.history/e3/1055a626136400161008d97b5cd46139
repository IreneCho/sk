<script>
	
	function fn() {
	/* 	param = {
			name : '홍길동',
			age : 30
		}
	 */	arg = {
			type : 'post',
			url : 'my5.jsp',
		//	data : param,
			dataType : 'html', //data를 xml파일로 만들어서 모두 보여줌
			success : JFn,
			error : errFn
		}
		$.ajax(arg)
	}
	function JFn(data) {
		console.log("확인",data)
	}
	function errFn(err) {
		console.log("에러",err)
	}
