<script src="http://code.jquery.com/jquery-3.1.0.js" integrity="sha256-slogkvB1K3VOkzAI8QITxV3VzpOnkeNVsKvtkYLMjfk=" crossorigin="anonymous"></script>
<script>

nCnt = 0

function fn(){
   my = setInterval( wan, 1000 )
}

function wan(){

   if( nCnt >= 1){
      
      $(".block").animate({"left": "-=1000px"}, "noraml");
      nCnt++
   }
   else if(nCnt == 10){
      nCnt = 0
   }

  $(".block").animate({"left": "+=1000px"}, "normal");
 
   nCnt++
}

function fn1(){
   clearInterval(my)   
}

</script>
</head>
<body>
 
<div class="container">
	<div id="imgBox">
	<div class="block"></div>
	</div>
	<div id="btnBox">
	<button onclick="fn()">시작</button>
	<button onclick="fn1()">중지</button>
	</div>
</div>

